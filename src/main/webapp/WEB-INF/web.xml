<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>MemberAuthenticationController</servlet-name>
        <servlet-class>
            chai.controllers.MemberAuthenticationController
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MemberAuthenticationController</servlet-name>
        <url-pattern>/authenticate-member</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AdminAuthenticationController</servlet-name>
        <servlet-class>
            chai.controllers.AdminAuthenticationController
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminAuthenticationController</servlet-name>
        <url-pattern>/authenticate-admin</url-pattern>
    </servlet-mapping>
</web-app>

